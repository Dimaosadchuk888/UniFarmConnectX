[[workflows.workflow]]
name = "Clean Start UniFarm"
author = 43232783
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pkill -f \"tsx server/index.ts\" || true"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "sleep 2"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "NODE_ENV=production PORT=3000 DATABASE_PROVIDER=neon FORCE_NEON_DB=true DISABLE_REPLIT_DB=true npx --yes tsx server/index.ts"

[[workflows.workflow]]
name = "Force Clean Start"
author = 43232783
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pkill -9 -f \"tsx server/index.ts\" || true"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pkill -9 -f \"npx tsx\" || true"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pkill -9 -f \"npm exec tsx\" || true"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "sleep 3"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "lsof -ti:3000 | xargs kill -9 || true"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "sleep 2"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "NODE_ENV=production PORT=3000 DATABASE_PROVIDER=neon FORCE_NEON_DB=true DISABLE_REPLIT_DB=true npx --yes tsx server/index.ts"


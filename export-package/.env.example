# Пример файла окружения для UniFarm в Cloud
# Создайте файл .env на основе этого примера

# Подключение к базе данных
# Обязательно измените на вашу строку подключения
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/unifarm

# Настройки приложения
NODE_ENV=production
PORT=3000

# Настройки базы данных
FORCE_NEON_DB=true
OVERRIDE_DB_PROVIDER=neon
DATABASE_PROVIDER=neon

# Настройки доступа
SKIP_TELEGRAM_CHECK=true
ALLOW_BROWSER_ACCESS=true

# Telegram Bot настройки
# Необходимо заменить на ваш токен!
# TELEGRAM_BOT_TOKEN=your_bot_token_here

# Безопасность сессий
SESSION_SECRET=change_this_to_a_random_secret_key

# Опциональные настройки
# DEBUG=false
# LOG_LEVEL=info